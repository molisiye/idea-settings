<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'Ctrl+C' or 'Ctrl+Insert'</ShortCut>
          <Description>Copy</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug">
        <Item>
          <IdeaActionID>ActivateDebugToolWindow</IdeaActionID>
          <ShortCut>'Alt+5'</ShortCut>
          <Description>Debug</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'PasswordUtil'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'Shift+F9'</ShortCut>
          <Description>Debug 'PasswordUtil'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'Unnamed'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'Shift+F9'</ShortCut>
          <Description>Debug 'Unnamed'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B'</ShortCut>
          <Description>Declaration</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Next / Move to Next Occurrence">
        <Item>
          <IdeaActionID>FindNext</IdeaActionID>
          <ShortCut>'F3' or 'Ctrl+L'</ShortCut>
          <Description>Find Next / Move to Next Occurrence</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Paste">
        <Item>
          <IdeaActionID>$Paste</IdeaActionID>
          <ShortCut>'Ctrl+V' or 'Shift+Insert'</ShortCut>
          <Description>Paste</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project Structure...">
        <Item>
          <IdeaActionID>ShowProjectStructureSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+S'</ShortCut>
          <Description>Project Structure...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'Evaluate'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'Evaluate'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'ReverseLIFO'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'ReverseLIFO'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>